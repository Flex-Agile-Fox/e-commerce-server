{
  "name": "e-commerce-server",
  "version": "1.0.0",
  "description": "e-commerce-server",
  "main": "index.js",
  "scripts": {
    "start": "node app.js",
    "test": "jest --runInBand --detectOpenHandles --forceExit",
    "dev": "nodemon app.js",
    "db:teardown": "sequelize-cli db:drop",
    "db:setup": "sequelize-cli db:create && sequelize-cli db:migrate && sequelize-cli db:seed:all",
    "db:magic": "npm run db:teardown && npm run db:setup && npm run dev",
    "db:setup:test": "sequelize-cli db:drop --env test && sequelize-cli db:create --env test && sequelize-cli db:migrate --env test && sequelize-cli db:seed:all --env test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ericksonManurung/e-commerce-server.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ericksonManurung/e-commerce-server/issues"
  },
  "homepage": "https://github.com/ericksonManurung/e-commerce-server#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.6.0",
    "sequelize": "^6.6.2"
  },
  "devDependencies": {
    "dotenv": "^10.0.0",
    "jest": "^27.0.3",
    "nodemon": "^2.0.7",
    "sequelize-cli": "^6.2.0",
    "supertest": "^6.1.3"
  }
}
